{
    "name": "yongtiger/yii2-admin",
    "description": "GUI manager for RABC (Role Base Access Control) Yii2. Easy to manage authorization of user",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","admin","rbac"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/yongtiger/yii2-admin/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "source": "https://github.com/yongtiger/yii2-admin"
    },
    "authors": [
        {
            "name": "Yong Tiger",
            "email": "tigeryang.brainbook@outlook.com",
            "homepage": "http://www.branbook.cc/yongtiger"
        }
    ],
    "homepage" : "http://www.branbook.cc",
    "require": {
        "php": ">=5.5",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*"
    },
    "suggest": {
        "yiisoft/yii2-bootstrap": "Used when using layout 'left-menu', 'right-menu' or 'top-menu'"
    },
    "autoload": {
        "psr-4": {
            "yongtiger\\admin\\": "src"
        }
    }
}
